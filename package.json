{
  "name": "POC",
  "version": "0.0.1",
  "private": true,
  "description": "POC for Dailymotion - Conitnuous playing experience offsite",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js --progress",
    "lint": "standard --verbose | snazzy",
    "start": "node tools/dev-server.js",
    "start:d": "webpack-dashboard -- npm start"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": [
              "last 2 versions",
              "> 1%",
              "safari >= 10"
            ],
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      "transform-class-properties",
      "transform-decorators-legacy"
    ]
  },
  "standard": {
    "env": {
      "browser": true,
      "node": true
    },
    "parser": "babel-eslint",
    "ignore": [
      "/doc/"
    ]
  },
  "keywords": [
    "node",
    "nodejs"
  ],
  "author": "Seb Pomel <sebastien.pomel@watchever.com>",
  "license": "No-License (see LICENSE file)",
  "dependencies": {
    "airbnb-prop-types": "^2.4.1",
    "autobind-decorator": "^1.3.3",
    "history": "^4.6.0"
  },
  "devDependencies": {
    "autoprefix": "^1.0.1",
    "autoprefixer": "^6.7.7",
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.2.0",
    "babel-loader": "^6.4.0",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.2.1",
    "babel-preset-react": "^6.23.0",
    "babel-preset-react-hmre": "^1.1.0",
    "babel-preset-stage-3": "^6.22.0",
    "body-parser": "^1.17.1",
    "colors": "^1.1.2",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^3.2.3",
    "express": "^4.15.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.1",
    "istanbul": "^0.4.3",
    "marked": "^0.3.6",
    "morgan": "^1.8.1",
    "offline-plugin": "^4.6.1",
    "postcss-loader": "^1.3.3",
    "rimraf": "^2.6.1",
    "snazzy": "^6.0.0",
    "standard": "^9.0.1",
    "style-loader": "^0.15.0",
    "url-loader": "^0.5.8",
    "webpack": "^2.2.1",
    "webpack-dashboard": "^0.3.0",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.17.1"
  }
}
